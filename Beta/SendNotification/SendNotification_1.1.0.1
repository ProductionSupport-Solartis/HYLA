swagger: '2.0'
info:
  version: "1.0.0"
  title: Solartis SendNotification API for HYLA
  description: To Send Notification for Device Insurance from Solartis
  
  
host: hylakb.uat.solartis.net
basePath: /KnowledgeEngineV6_6/KnowledgeBase
schemes:
  - https
consumes:
  - application/json
produces:
  - application/json
paths:
  /FireEventV3:
    post:
      description: Send Notification for the given input
      operationId: SendNotification
      produces:
        - application/json
      parameters:
        - name: Token
          in: header
          description: Authentication Token
          required: true
          type: string
          format: string
          default: ""
        - name: EventName
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "SendNotification_1.1.0.1"
        - name: OwnerId
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "2282"
        - name: MODE
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "DEV"
        - name: Environment
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "15"
        - name: SendNotifications
          in: body
          description: Sends Notifications
          required: true
          schema:
            $ref: '#/definitions/SendNotification_Request'
      responses:
        '200':
          description: Success Response
          schema:
            $ref: '#/definitions/SendNotification_Success_Response'
        default:
          description: unexpected error
          schema:
            $ref: '#/definitions/errorModel'

definitions:
  SendNotification_Request:
    type: object
    required:
      - ServiceRequestDetail
      - dynamicInformation
      - deviceInformation

    properties:
      ServiceRequestDetail:
        type: object
        properties:
          OwnerId:
            type: string
            default: "2282"
          ResponseType:
            type: string
            enum: ["JSON","XML"]
            default: "JSON"
          EndClientUserUniqueSessionId:
            type: string
            default: "1.0"
          UserName:
            type: string
            default: "hylaadmin"
          Token:
            type: string
            default: ""
          ServiceRequestVersion:
            type: string
            default: "1.0"
          ServiceResponseVersion:
            type: string
            default: "1.0"
          RegionCode:
            type: string
            default: "US"
          LanguageCode:
            type: string
            default: "en"
      accountId:
        type: string
        default: "1021140"
      notificationId:
        type: string
        default: "HYNOT1"
      dynamicInformation:
        type: array
        items:
          properties:
            Value:
              type: string
              default: "10000"
            Key:
              type: string
              default: "dynamic attribute 1"
      deviceInformation:
        type: array
        items:
          properties:
            deviceId:
              type: string
              default: "2030577"
            loss:
              type: array
              items:
                properties:
                  claim:
                    type: array
                    items:
                      properties:
                        claimID: 
                          type: string
                          default: "HYLACL0001"

  SendNotification_Success_Response:
    type: object
    required:
      - ServiceRequestDetail
      - dynamicInformation
      - deviceInformation

    properties:
      ServiceRequestDetail:
        type: object
        properties:
          OwnerId:
            type: string
            default: "2282"
          ResponseType:
            type: string
            enum: ["JSON","XML"]
            default: "JSON"
          EndClientUserUniqueSessionId:
            type: string
            default: "1.0"
          UserName:
            type: string
            default: "hylaadmin"
          Token:
            type: string
            default: ""
          ServiceRequestVersion:
            type: string
            default: "1.0"
          ServiceResponseVersion:
            type: string
            default: "1.0"
          RegionCode:
            type: string
            default: "US"
          LanguageCode:
            type: string
            default: "en"
      accountId:
        type: string
        default: "123243"
      notificationId:
        type: string
        default: "HYNOT1"
      responseStatus:
        type: string
        default: "Success"
      dynamicInformation:
        type: array
        items:
          properties:
            Value:
              type: string
              default: "10000"
            Key:
              type: string
              default: "dynamic attribute 1"
      deviceInformation:
        type: array
        items:
          properties:
            deviceId:
              type: string
              default: "2030577"
            loss:
              type: array
              items:
                properties:
                  claim:
                    type: array
                    items:
                      properties:
                        claimID: 
                          type: string
                          default: "HYLACL0001"

            
  errorModel:
    type: object
    required:
      - RequestStatus
      - MessageDetail
    properties:
      RequestStatus:
        type: string
      MessageDetail:
        type: object
        properties:
          MessageCode:
            type: string
          MessageType:
            type: string
          UserMessage:
            type: string
          MessageLevel:
            type: string
          MessageSeverity:
            type: string
          MessageList:
            type: object
            properties:
              Message:
                type: array
                items:
                  type: string       
