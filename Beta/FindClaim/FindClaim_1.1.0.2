swagger: '2.0'
info:
  version: "1.0.0"
  title: Solartis FindClaim API for HYLA
  description: To Find account details
  
  
host: hylakb.uat.solartis.net
basePath: /KnowledgeEngineV6_6/KnowledgeBase
schemes:
  - https
consumes:
  - application/json
produces:
  - application/json
paths:
  /FireEventV3:
    post:
      description: To find Claim details for the given input
      operationId: FindClaim
      produces:
        - application/json
      parameters:
        - name: Token
          in: header
          description: Authentication Token
          required: true
          type: string
          format: string
          default: ""
        - name: EventName
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "FindClaim_1.1.0.2"
        - name: OwnerId
          in: header
          description: Owner ID of the Product
          required: true
          type: string
          format: string
          default: "2282"
        - name: MODE
          in: header
          description: MODE of the product
          required: true
          type: string
          format: string
          default: "LIVE"
        - name: Environment
          in: header
          description: Environment in which the event to be invoked
          required: true
          type: string
          format: string
          default: "15"
        - name: FindClaim
          in: body
          description: Find Claim Details
          required: true
          schema:
            $ref: '#/definitions/FindClaim_Request'
      responses:
        '200':
          description: Success Response
          schema:
            $ref: '#/definitions/FindClaim_Success_Response'
        default:
          description: unexpected error
          schema:
            $ref: '#/definitions/errorModel'

definitions:
  FindClaim_Request:
    type: object
    required:
      - ServiceRequestDetail
    properties:
      ServiceRequestDetail:
        type: object
        properties:
          OwnerId:
            type: string
            default: "2282"
          ResponseType:
            type: string
            enum: ["JSON","XML"]
            default: "JSON"
          EndClientUserUniqueSessionId:
            type: string
            default: "1.0"
          UserName:
            type: string
            default: "hylaadmin"
          Token:
            type: string
            default: ""
          ServiceRequestVersion:
            type: string
            default: "1.0"
          ServiceResponseVersion:
            type: string
            default: "1.0"
          RegionCode:
            type: string
            default: "US"
          LanguageCode:
            type: string
            default: "en"
         
      accountId:
        type: string
        default: "1021140"
      deviceId:
        type: string
        default: "2030577"
      claimId: 
        type: string
        default: "HYLACL-14428"
      firstName: 
        type: string
        default: "Sanjay"
      lastName: 
        type: string
        default: "Ramesh"
      claimReason: 
        type: string
        default: "DEVICE_LOST"
      claimType: 
        type: string
        default: "PRORATA"
      claimStatus: 
        type: string
        default: "Success"
      repairOption:
        type: string
        default: "Concierge"
      createdFromDate:
        type: string
        default: "09/27/2021"
      createdToDate:
        type: string
        default: "09/27/2021"
      serviceRequestId:
        type: string
        default: "201584"

  FindClaim_Success_Response:
    type: object
    required:
      - ServiceRequestDetail
      - claimList
    properties:
      ServiceRequestDetail:
        type: object
        properties:
          ResponseType:
            type: string
            enum: ["JSON","XML"]
            default: "JSON"
          OwnerId:
            type: string
            default: "2282"
          EndClientUserUniqueSessionId:
            type: string
            default: "1.0"
          UserName:
            type: string
            default: "hylaadmin"
          Token:
            type: string
            default: ""
          ServiceRequestVersion:
            type: string
            default: "1.0"
          RegionCode:
            type: string
            default: "US"
      ResponseStatus:
        type: string
     
      claimList:
        type: array
        items:
          properties:
            accountId:
              type: string
              default: "1021140"
            deviceId:
              type: string
              default: "2030577"
            claimId: 
              type: string
              default: "HYLACL-14428"
            firstName: 
              type: string
              default: "Sanjay"
            lastName: 
              type: string
              default: "Ramesh"
            claimReason: 
              type: string
              default: "DEVICE_LOST"
            claimType: 
              type: string
              default: "PRORATA"
            claimStatus: 
              type: string
              default: "Success"
            claimCreatedDate:
              type: string
              default: ""
            serviceRequestId:
              type: string
              default: "09/27/2021"
            repairOption:
              type: string
              default: "09/27/2021"
             
 
  
  errorModel:
    type: object
    required:
      - RequestStatus
      - MessageDetail
    properties:
      RequestStatus:
        type: string
      MessageDetail:
        type: object
        properties:
          MessageCode:
            type: string
          MessageType:
            type: string
          UserMessage:
            type: string
          MessageLevel:
            type: string
          MessageSeverity:
            type: string
          MessageList:
            type: object
            properties:
              Message:
                type: array
                items:
                  type: string       
