swagger: '2.0'
info:
  version: "1.0.0"
  title: Solartis GetDeviceCatalogData API for HYLA
  description: To Get device catalog data for Device Insurance from Solartis
  
  
host: hylakb.uat.solartis.net
basePath: /KnowledgeEngineV6_6/KnowledgeBase
schemes:
  - https
consumes:
  - application/json
produces:
  - application/json
paths:
  /FireEventV3:
    post:
      description: Get device catalog data for the given input
      operationId: getDeviceCatalogData
      produces:
        - application/json
      parameters:
        - name: Token
          in: header
          description: Authentication Token
          required: true
          type: string
          format: string
          default: ""
        - name: EventName
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "GetDeviceCatalog_1.1.0.2"
        - name: OwnerId
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "2282"
        - name: MODE
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "DEV"
        - name: Environment
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "15"
        - name: GetDeviceCatalog
          in: body
          description: Get Device Catalog
          required: true
          schema:
            $ref: '#/definitions/GetDeviceCatalogData_Request'
      responses:
        '200':
          description: Success Response
          schema:
            $ref: '#/definitions/GetDeviceCatalogData_Success_Response'
        default:
          description: unexpected error
          schema:
            $ref: '#/definitions/errorModel'

definitions:
  GetDeviceCatalogData_Request:
    type: object
    required:
      - ServiceRequestDetail
      - DeviceCatalog
    properties:
      ServiceRequestDetail:
        type: object
        properties:
          OwnerId:
            type: string
            default: "2282"
          ResponseType:
            type: string
            enum: ["JSON","XML"]
            default: "JSON"
          EndClientUserUniqueSessionId:
            type: string
            default: "1.0"
          UserName:
            type: string
            default: "hylaadmin"
          Token:
            type: string
            default: ""
          ServiceRequestVersion:
            type: string
            default: "1.0"
          ServiceResponseVersion:
            type: string
            default: "1.0"
          RegionCode:
            type: string
            default: "US"
          LanguageCode:
            type: string
            default: "en"
      DeviceCatalog:
        type: array
        items:
          properties:
            productId:
              type: string
              default: "1334613"
            modelId:
              type: string
              default: "1268171"
            manufacturer:
              type: string
              default: "Apple"
            modelName:
