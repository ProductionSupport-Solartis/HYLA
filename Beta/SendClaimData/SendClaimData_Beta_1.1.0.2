paths:
  /FireEventV3:
    post:
      description: Send claims data for the given input
      operationId: sendClaimsData
      produces:
        - application/json
      parameters:
        - name: Token
          in: header
          description: Authentication Token
          required: true
          type: string
          format: string
          default: ""
        - name: EventName
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "SendClaimData_1.1.0.2"
        - name: OwnerId
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "2282"
        - name: MODE
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "DEV"
        - name: Environment
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "15"
        - name: SendClaimData
          in: body
          description: SendClaimsData
          required: true
          schema:
            $ref: '#/definitions/SendClaimData_Request'
      responses:
        '200':
          description: Success Response
          schema:
            $ref: '#/definitions/SendClaimData_Success_Response'
        default:
          description: unexpected error
          schema:
            $ref: '#/definitions/errorModel'

