swagger: '2.0'
info:
  version: "1.0.0"
  title: Solartis AddDevice API for HYLA
  description: To add device for Device Insurance
  
host: hylakb.uat.solartis.net
basePath: /KnowledgeEngineV6_6/KnowledgeBase
schemes:
  - https
consumes:
  - application/json
produces:
  - application/json
paths:
  /FireEventV3:
    post:
      description: To add device for the given input
      operationId: AddDevice
      produces:
        - application/json
      parameters:
        - name: Token
          in: header
          description: Authentication Token
          required: true
          type: string
          format: string
          default: ""
        - name: EventName
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: AddDevice_1.1.0.1
        - name: OwnerId
          in: header
          description: Owner ID of the Product
          required: true
          type: string
          format: string
          default: "2282"
        - name: MODE
          in: header
          description: MODE of the product
          required: true
          type: string
          format: string
          default: "DEV"
        - name: Environment
          in: header
          description: Environment in which the event to be invoked
          required: true
          type: string
          format: string
          default: "15"
        - name: Add Device
          in: body
          description: Add Device
          required: true
          schema:
            $ref: '#/definitions/AddDevice_Request'
      responses:
        '200':
          description: Success Response
          schema:
            $ref: '#/definitions/AddDevice_Success_Response'
        default:
          description: unexpected error
          schema:
            $ref: '#/definitions/errorModel'

definitions:
  AddDevice_Request:
    type: object
    required:
      - ServiceRequestDetail
    properties:
      ServiceRequestDetail:
        type: object
        properties:
          OwnerId:
            type: string
            default: "2282"
          ResponseType:
            type: string
            enum: ["JSON","XML"]
            default: "JSON"
          EndClientUserUniqueSessionId:
            type: string
            default: "1.0"
          UserName:
            type: string
            default: "hylaadmin"
          Token:
            type: string
            default: ""
          ServiceRequestVersion:
            type: string
            default: "1.0"
          ServiceResponseVersion:
            type: string
            default: "1.0"
          RegionCode:
            type: string
            default: "US"
          LanguageCode:
            type: string
            default: "en"
      accountId:
        type: string
        default: "1009951"
   
      deviceInformation:
        type: array
        items: 
          properties:
            deviceNickName: 
              type: string
              default: "iPhone X"
            devicePhoneNumber: 
              type: string
              default: "+91 8778156318"
            make: 
              type: string
              default: "Apple"
            model: 
              type: string
              default: "iPhone 11"
            color: 
              type: string
              default: "Red"
            capacity: 
              type: string
              default: "256GB"
            carrier: 
              type: string
              default: "Verizon"
            tier: 
              type: string
              default: "6"
            imei: 
              type: string
              default: "329498988990501"

  AddDevice_Success_Response:
    type: object
    required:
      - ServiceRequestDetail
    properties:
      ServiceRequestDetail:
        type: object
        properties:
          ResponseType:
            type: string
            enum: ["JSON","XML"]
            default: "JSON"
          OwnerId:
            type: string
            default: "2282"
          EndClientUserUniqueSessionId:
            type: string
            default: "1.0"
          UserName:
            type: string
            default: "hylaagent"
          Token:
            type: string
            default: ""
          ServiceRequestVersion:
            type: string
            default: "1.0"
          RegionCode:
            type: string
            default: "US"
      ResponseStatus:
              type: string
              default: "Success"
      deviceInformation:
        type: object
        properties:
            deviceId:
              type: string
              default: "2003963"
            deviceNickName: 
              type: string
              default: "iPhone X"
            devicePhoneNumber:
              type: string
              default: "+91 8778156318"
            make:
              type: string
              default: "Apple"
            model: 
              type: string
              default: "iPhone 11"
            color: 
              type: string
              default: "Red"
            capacity: 
              type: string
              default: "256GB"
            carrier: 
              type: string
              default: "US Cellular"
            tier: 
              type: string
              default: "6"
            imei: 
              type: string
              default: "329497988990521"

  errorModel:
    type: object
    required:
      - RequestStatus
      - MessageDetail
    properties:
      RequestStatus:
        type: string
      MessageDetail:
        type: object
        properties:
          MessageCode:
            type: string
          MessageType:
            type: string
          UserMessage:
            type: string
          MessageLevel:
            type: string
          MessageSeverity:
            type: string
          MessageList:
            type: object
            properties:
              Message:
                type: array
                items:
                  type: string       
