swagger: '2.0'
info:
  version: "1.0.0"
  title: Solartis CreateQuote API for HYLA
  description: To Create quote for Device Insurance
  
  
host: hylakb.uat.solartis.net
basePath: /KnowledgeEngineV6_6/KnowledgeBase
schemes:
  - https
consumes:
  - application/json
produces:
  - application/json
paths:
  /FireEventV3:
    post:
      description: To create Quote for the given input
      operationId: CreateQuote
      produces:
        - application/json
      parameters:
        - name: Token
          in: header
          description: Authentication Token
          required: true
          type: string
          format: string
          default: ""
        - name: EventName
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "CreateQuote_1.1.0.9"
        - name: OwnerId
          in: header
          description: Owner ID of the Product
          required: true
          type: string
          format: string
          default: "2282"
        - name: MODE
          in: header
          description: MODE of the product
          required: true
          type: string
          format: string
          default: "LIVE"
        - name: Environment
          in: header
          description: Environment in which the event to be invoked
          required: true
          type: string
          format: string
          default: "15"
        - name: CreateQuote
          in: body
          description: Create Quote
          required: true
          schema:
            $ref: '#/definitions/CreateQuote_Request'
      responses:
        '200':
          description: Success Response
          schema:
            $ref: '#/definitions/CreateQuote_Success_Response'
        default:
          description: unexpected error
          schema:
            $ref: '#/definitions/errorModel'

