swagger: '2.0'
info:
  version: "1.0.0"
  title: Solartis SubmitClaim API for HYLA
  description: To Submit claim for the Device added in the Account
  
  
host: hylakb.uat.solartis.net
basePath: /KnowledgeEngineV6_6/KnowledgeBase
schemes:
  - https
consumes:
  - application/json
produces:
  - application/json
paths:
  /FireEventV3:
    post:
      description: Submit Claims for the given input
      operationId: submitClaims
      produces:
        - application/json
      parameters:
        - name: Token
          in: header
          description: Authentication Token
          required: true
          type: string
          format: string
          default: ""
        - name: EventName
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "SubmitClaim_1.1.0.8"
        - name: OwnerId
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "2282"
        - name: MODE
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "LIVE"
        - name: Environment
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "15"
        - name: SubmitClaim
          in: body
          description: Plan Details
          required: true
          schema:
            $ref: '#/definitions/SubmitClaim_Request'
      responses:
        '200':
          description: Success Response
          schema:
            $ref: '#/definitions/SubmitClaim_Success_Response'
        default:
          description: unexpected error
          schema:
            $ref: '#/definitions/errorModel'

definitions:
  SubmitClaim_Request:
    type: object
    required:
      - ServiceRequestDetail
      - deviceInformation
    properties:
      ServiceRequestDetail:
        type: object
        properties:
          OwnerId:
            type: string
            default: "2282"
          ResponseType:
            type: string
            enum: ["JSON","XML"]
            default: "JSON"
          EndClientUserUniqueSessionId:
            type: string
            default: "1.0"
          UserName:
            type: string
            default: "hylaadmin"
          Token:
            type: string
            default: ""
          ServiceRequestVersion:
            type: string
            default: "1.0"
          ServiceResponseVersion:
            type: string
            default: "1.0"
          RegionCode:
            type: string
            default: "US"
          LanguageCode:
            type: string
            default: "en"
      accountId:
            type: string
            default: "0001"
     
      deviceInformation:
        type: array
        items:
          properties:
            deviceId:
              title: "Device ID"
              type: string
              default: "2030577"
            deviceNickName:
              title: "Device Nickname"
              type: string
              default: "Iphone"
            loss:
              type: array
              title: "Loss"
              items:
                properties:
                  dateOfLoss:
                    title: "Date Of Loss"
                    type: string
                    default: "08/24/2021"
                  claim:
                    title: "Claim"
                    type: array
                    items:
                      properties:
                        claimReasons:
                          title: "Claim Reason"
                          type: string
                          enum: ["LOST", "STOLEN", "SCREEN", "BATTERY", "BUTTONS", "CHARGING_PORT",    "WATER_DAMAGE","WIFI_ANTENNA","BLUETOOTH","SCREEN_RMB","CAMERA","MICROPHONE","BACK_GLASS","ACCELEROMETER","SPEAKER","OTHER","BATTERY_RMB"]
                          default: "LOST"
                        claimCause:
                          title: "CLAIM CAUSE"
                          type: string
                          default: "I Lost my phone while travelling"
                        communicationPreferences:
                          title: "Communication Preferences"
                          type: string
                          enum: ["EMAIL", "EMAIL AND SMS"]
                        additionalCommunicationEmail:
                          title: "Additional Communication Email"
                          type: string
                        additionalCommunicationPhone:
                          title: "Additional Communication Phone"
                          type: string
                        claimCreatedDt:
                          title: "Claim Created Date"
                          type: string
                          default: "07/16/2021 11:11:11"
                          
                        

  SubmitClaim_Success_Response:
    type: object
    required:
      - ServiceRequestDetail
      - deviceInformation
    properties:
      
      ServiceRequestDetail:
        type: object
        properties:
          ResponseType:
            type: string
            enum: ["JSON","XML"]
            default: "JSON"
          OwnerId:
            type: string
            default: "2282"
          EndClientUserUniqueSessionId:
            type: string
            default: "1.0"
          UserName:
            type: string
            default: "hylaagent"
          Token:
            type: string
            default: ""
          ServiceRequestVersion:
            type: string
            default: "1.0"
          ServiceResponseVersion:
            type: string
            default: "1.0"
          RegionCode:
            type: string
            default: "US"
          LanguageCode:
            type: string
            default: "en"
      accountId:
            type: string
            default: "1021140"
      deviceInformation:
        type: array
        items:
          properties:
            deviceId:
              title: "deviceId"
              type: string
              default: "2030577"
            deviceNickName:
              title: "deviceNickName"
              type: string
              default: "iphone"
            loss:
              type: array
              items:
                properties:
                  dateOfLoss:
                    type: string
                    default: "08/24/2021"
                  claim:
                    type: array
                    items:
                      properties:
                        claimReasons:
                          type: string
                        claimCause:
                          type: string
                        claimId:
                          type: string
                          default: HYLACL-15428"
                        claimStatus:
                          type: string
                          default: "claimed"
                        claimState:
                          type: string
                          default: "NE"
                        claimType:
                          type: string
                          default: "Repair"
                        claimCreatedDt:
                          type: string
                        claimDenialCode:
                          type: string
                        claimDenialReason:
                          type: string
                        canBeCancelled:
                          type: string
                        deductibleAmount:
                          type: string
                          default: "99.00"
                        serviceFee:
                          type: string
                          default: "99.00"
                        screenCrackServiceFee:
                          type: string
                        communicationPreferences:
                          type: string
                        additionalCommunicationEmail:
                          title: "Additional Communication Email"
                          type: string
                          default: "bhavana_a@solartis.com"
                        additionalCommunicationPhone:
                          title: "Additional Communication Phone"
                          type: string
                          default: "+91 8667724046"
                  
                       
  errorModel:
    type: object
    required:
      - RequestStatus
      - MessageDetail
    properties:
      RequestStatus:
        type: string
      MessageDetail:
        type: object
        properties:
          MessageCode:
            type: string
          MessageType:
            type: string
          UserMessage:
            type: string
          MessageLevel:
            type: string
          MessageSeverity:
            type: string
          MessageList:
            type: object
            properties:
              Message:
                type: array
                items:
                  type: string       
