swagger: '2.0'
info:
  version: "1.0.0"
  title: Solartis ResetPassword API for HYLA
  description: To Reset the Password  
  
  
host: hylakb.uat.solartis.net
basePath: /KnowledgeEngineV6_6/KnowledgeBase
schemes:
  - https
consumes:
  - application/json
produces:
  - application/json
paths:
  /FireEventV3:
    post:
      description: To Reset the Password for the given input
      operationId: ResetPassword
      produces:
        - application/json
      parameters:
        - name: Token
          in: header
          description: Authentication Token
          required: true
          type: string
          format: string
          default: ""
        - name: EventName
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "ResetPassword_1.1.0.1"
        - name: OwnerId
          in: header
          description: Owner ID of the Product
          required: true
          type: string
          format: string
          default: "2282"
        - name: MODE
          in: header
          description: MODE of the product
          required: true
          type: string
          format: string
          default: "DEV"
        - name: Environment
          in: header
          description: Environment in which the event to be invoked
          required: true
          type: string
          format: string
          default: "15"
        - name: Reset Password
          in: body
          description: Reset Password
          required: true
          schema:
            $ref: '#/definitions/ResetPassword_Request'
      responses:
        '200':
          description: Success Response
          schema:
            $ref: '#/definitions/ResetPassword_Success_Response'
        default:
          description: unexpected error
          schema:
            $ref: '#/definitions/errorModel'

definitions:
  ResetPassword_Request:
    type: object
    required:
      - ServiceRequestDetail
      - loginCredentials
    properties:
      ServiceRequestDetail:
        type: object
        properties:
          OwnerId:
            type: string
            default: "2282"
          ResponseType:
            type: string
            enum: ["JSON","XML"]
            default: "JSON"
          EndClientUserUniqueSessionId:
            type: string
            default: "1.0"
          UserName:
            type: string
            default: "hylaadmin"
          Token:
            type: string
            default: ""
          ServiceRequestVersion:
            type: string
            default: "1.0"
          ServiceResponseVersion:
            type: string
            default: "1.0"
          RegionCode:
            type: string
            default: "US"
          LanguageCode:
            type: string
            default: "en"
      loginCredentials:
        type: object
        properties:
            userId: 
              type: string
              default: "john@gmail.com"
            resetPasswordURL: 
              type: string
              default: "https://hyla.uat.solartis.net/ResetPassword/HylaDemo/"

      
  ResetPassword_Success_Response:
    type: object
    required:
      - ServiceRequestDetail
      - loginCredentials
    properties:
      ServiceRequestDetail:
        type: object
        properties:
          ResponseType:
            type: string
            enum: ["JSON","XML"]
            default: "JSON"
          OwnerId:
            type: string
            default: "2282"
          EndClientUserUniqueSessionId:
            type: string
            default: "1.0"
          UserName:
            type: string
            default: "hylaagent"
          Token:
            type: string
            default: ""
          BrowserIp:
            type: string 
            default: "127.0.0.1"
          ServiceRequestVersion:
            type: string
            default: "1.0"
          RegionCode:
            type: string
            default: "US"
      ResponseStatus:
        type: string
      loginCredentials: 
        type: object
        properties:
            userId: 
              type: string

  errorModel:
    type: object
    required:
      - RequestStatus
      - MessageDetail
    properties:
      RequestStatus:
        type: string
      MessageDetail:
        type: object
        properties:
          MessageCode:
            type: string
          MessageType:
            type: string
          UserMessage:
            type: string
          MessageLevel:
            type: string
          MessageSeverity:
            type: string
          MessageList:
            type: object
            properties:
              Message:
                type: array
                items:
                  type: string       
