swagger: '2.0'
info:
  version: "1.0.0"
  title: Solartis Issue Renewal Policy API for HYLA
  description: To make payment for unpaid renewal quotes
  
  
host: hylakb.uat.solartis.net
basePath: /KnowledgeEngineV6_6/KnowledgeBase
schemes:
  - https
consumes:
  - application/json
produces:
  - application/json
paths:
  /FireEventV3:
    post:
      description: To make payment for unpaid renewal quotes
      operationId: IssueRenewalPolicy
      produces:
        - application/json
      parameters:
        - name: Token
          in: header
          description: Authentication Token
          required: true
          type: string
          format: string
          default: ""
        - name: EventName
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "IssueRenewalPolicy_1.1.0.1"
        - name: OwnerId
          in: header
          description: Owner ID of the Product
          required: true
          type: string
          format: string
          default: "2282"
        - name: MODE
          in: header
          description: MODE of the product
          required: true
          type: string
          format: string
          default: "LIVE"
        - name: Environment
          in: header
          description: Environment in which the event to be invoked
          required: true
          type: string
          format: string
          default: "15"
        - name: CreateRenewalPolicy
          in: body
          description: Aurtorenewal of polcies
          required: true
          schema:
            $ref: '#/definitions/IssueRenewalPolicy_Request'
      responses:
        '200':
          description: Success Response
          schema:
            $ref: '#/definitions/IssueRenewalPolicy_Success_Response'
        default:
          description: unexpected error
          schema:
            $ref: '#/definitions/errorModel'

definitions:
  IssueRenewalPolicy_Request:
    type: object
    required:
      - ServiceRequestDetail
      - deviceInformation
    properties:
      ServiceRequestDetail:
        type: object
        properties:
          OwnerId:
            type: string
            default: "2282"
          ResponseType:
            type: string
            enum: ["JSON","XML"]
            default: "JSON"
          EndClientUserUniqueSessionId:
            type: string
            default: "1.0"
          UserName:
            type: string
            default: "hylaadmin"
          Token:
            type: string
            default: ""
          ServiceRequestVersion:
            type: string
            default: "1.0"
          ServiceResponseVersion:
            type: string
            default: "1.0"
          RegionCode:
            type: string
            default: "US"
          LanguageCode:
            type: string
            default: "en"
      accountId:
        type: string
        default: "1003312"
      deviceInformation:
        type: array
        items: 
          properties:
            policyNumber:
              type: string
              default: "POL-17922"
              
              
  IssueRenewalPolicy_Success_Response:
    type: object
    required:
      - ServiceRequestDetail
      - deviceInformation
    properties:
      ServiceRequestDetail:
        type: object
        properties:
          ResponseType:
            type: string
            enum: ["JSON","XML"]
            default: "JSON"
          OwnerId:
            type: string
            default: "2282"
          EndClientUserUniqueSessionId:
            type: string
            default: "1.0"
          UserName:
            type: string
            default: "hylaadmin"
          Token:
            type: string
            default: ""
          ServiceRequestVersion:
            type: string
            default: "1.0"
          ServiceResponseVersion:
            type: string
            default: "1.0"
          RegionCode:
            type: string
            default: "US"
          LanguageCode:
            type: string
            default: "en"
      ResponseStatus:
        type: string
      responseCode:
        type: string
      accountId:
        type: string
      emailNotificationsent:
        type: string
      documents:
        type: object
        properties:
            name:
              type: string
              default: "Invoice"
            URL:
              type: string
      deviceInformation:
        type: array
        items:
          properties:
            deviceId:
              type: string
            quoteList:
              type: array
              items:
                properties:
                  documents:
                    type: object
                    properties:
                      name:
                        type: string
                      URL:
                        type: string

                  policyNumber:
                    type: string
                  quoteNumber:
                    type: string
                  devicePremium:
                    type: string
                  planDetails:
                    type: array
                    items:
                      properties:
                        type:
                          type: string
                        policyNumber:
                          type: string
                        documents:
                          type: array
                          items:
                            properties:
                              name:
                                type: string
                              URL:
                                type: string 
  errorModel:
    type: object
    required:
      - RequestStatus
      - MessageDetail
    properties:
      RequestStatus:
        type: string
      MessageDetail:
        type: object
        properties:
          MessageCode:
            type: string
          MessageType:
            type: string
          UserMessage:
            type: string
          MessageLevel:
            type: string
          MessageSeverity:
            type: string
          MessageList:
            type: object
            properties:
              Message:
                type: array
                items:
                  type: string       
