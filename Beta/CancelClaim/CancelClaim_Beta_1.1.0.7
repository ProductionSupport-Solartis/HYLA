swagger: '2.0'
info:
  version: "1.0.0"
  title: Solartis Cancel Claim API for HYLA
  description: To cancel claim for Device Insurance from Solartis
host: hylakb.uat.solartis.net
basePath: /KnowledgeEngineV6_6/KnowledgeBase
schemes:
  - https
consumes:
  - application/json
produces:
  - application/json
paths:
  /FireEventV3:
    post:
      description: Cancel Claim for the given input
      operationId: cancelClaim
      produces:
        - application/json
      parameters:
        - name: Token
          in: header
          description: Authentication Token
          required: true
          type: string
          format: string
          default: ""
        - name: EventName
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "CancelClaim_1.1.0.7"
        - name: OwnerId
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "2282"
        - name: MODE
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "LIVE"
        - name: Environment
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "15"
        - name: CancelClaim
          in: body
          description: CancelClaim
          required: true
          schema:
            $ref: '#/definitions/CancelClaim_Request'
      responses:
        '200':
          description: Success Response
          schema:
            $ref: '#/definitions/CancelClaim_Success_Response'
        default:
          description: unexpected error
          schema:
            $ref: '#/definitions/errorModel'

definitions:
  CancelClaim_Request:
    type: object
    required:
      - ServiceRequestDetail
    properties:
      ServiceRequestDetail:
        type: object
        properties:
          OwnerId:
            type: string
            default: "2282"
          ResponseType:
            type: string
            enum: ["JSON","XML"]
            default: "JSON"
          EndClientUserUniqueSessionId:
            type: string
            default: "1.0"
          UserName:
            type: string
            default: "hylaadmin"
          Token:
            type: string
            default: ""
          ServiceRequestVersion:
            type: string
            default: "1.0"
          ServiceResponseVersion:
            type: string
            default: "1.0"
          RegionCode:
            type: string
            default: "US"
          LanguageCode:
            type: string
            default: "en"
      accountId:
        type: string
        default: "1021140"
      deviceInformation:
        type: array
        items:
          properties:
            deviceId:
              title: "Device ID"
              type: string
              default: "2030577"
            loss:
              type: array
              items:
                properties:
                 claim:
                  type: array
                  items:
                    properties:
                      claimId:
                        title: "Claim ID"
                        type: string
                        default: "HYLACL-0115"
                      cancellationReason:
                        title: "Cancellation Reason"
                        type: string
                        enum: ["User denies to deactivate the device","User denies to pay the deductible","Deductible Payment failure","User Request","Underwriting Reasons","User denies to return device","Found phone","Got new phone","Repair too long","Repaired elsewhere","Replaced phone","Customer Inactivity"]
                      cancellationDescription:
                        title: "Cancellation Description"
                        type: string
                      claimCancelledDt:
                        title: "Claim Cancelled Date"
                        type: string
                        default: "04/14/2020 11:11:11"
                        
                      
  CancelClaim_Success_Response:
    type: object
    required:
      - ServiceRequestDetail
      - deviceInformation
    properties:
      ServiceRequestDetail:
        type: object
        properties:
          ResponseType:
            type: string
            enum: ["JSON","XML"]
            default: "JSON"
          OwnerId:
            type: string
            default: "2282"
          EndClientUserUniqueSessionId:
            type: string
            default: "1.0"
          UserName:
            type: string
            default: "hylaagent"
          Token:
            type: string
            default: ""
          BrowserIp:
            type: string 
            default: "127.0.0.1"
          ServiceRequestVersion:
            type: string
            default: "1.0"
          RegionCode:
            type: string
            default: "US"
      accountId:
         type: string
         default: "1021140"
      ResponseStatus:
        type: string
      documents:
         type: array
         items:
           properties: 
             name:
               title: "name"
               type: string
             URL:
               title: "URL"
               type: string 
      
      deviceInformation:
        type: array
        items:
          properties:
            deviceId:
              title: "deviceId"
              type: string
              default: "2030577"
            loss:
              type: array
              items:
                properties:
                 claim:
                  type: array
                  items:
                    properties:
                      claimId:
                        title: "claimId"
                        type: string
                        default: "HYLACL-14428"
                      claimStatus:
                        title: "claimStatus"
                        type: string
                        default: "Success"
                      claimState:
                        title: "claimState"
                        type: string
                        default: "NE"
                      claimType:
                        title: "claimType"
                        type: string
                        default: "Broken"
                      claimCreatedDate:
                        title: "claimCreatedDate"
                        type: string
                        default: "09/27/2021"
                      deductibleAmount:
                        title: "deductibleAmount"
                        type: string
                        default: "9.65"
                      cancellationDescription:
                        title: "cancellationDescription"
                        type: string
                        default: "User denies to deactivate the device"
                      cancellationReason:
                        title: "cancellationReason"
                        type: string
                        default: "User denies to deactivate the device"
                      claimCancelledDt:
                        title: "claimCancelledDt"
                        type: string
                      settlementDetail:
                        type: object
                        properties:
                          refundTransactionId:
                            title: "refundTransactionId"
                            type: string
                          refundTrancationStatus:
                            title: "refundTrancationStatus"
                            type: string
  errorModel:
    type: object
    required:
      - RequestStatus
      - MessageDetail
    properties:
      RequestStatus:
        type: string
      MessageDetail:
        type: object
        properties:
          MessageCode:
            type: string
          MessageType:
            type: string
          UserMessage:
            type: string
          MessageLevel:
            type: string
          MessageSeverity:
            type: string
          MessageList:
            type: object
            properties:
              Message:
                type: array
                items:
                  type: string       
