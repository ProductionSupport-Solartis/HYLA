swagger: '2.0'
info:
  version: "1.0.2"
  title: Solartis CancelDevicePlan API for HYLA
  description: To cancel the existing Plans for the Device 
  
  
host: hylakb.uat.solartis.net
basePath: /KnowledgeEngineV6_6/KnowledgeBase
schemes:
  - https
consumes:
  - application/json
produces:
  - application/json
paths:
  /FireEventV3:
    post:
      description: Cancel the existing plans for the given input
      operationId: cancelDevicePlan
      produces:
        - application/json
      parameters:
        - name: Token
          in: header
          description: Authentication Token
          required: true
          type: string
          format: string
          default: ""
        - name: EventName
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "CancelDevicePlan_1.1.0.7"
        - name: OwnerId
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "2282"
        - name: MODE
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "LIVE"
        - name: Environment
          in: header
          description: Name of the event to be invoked
          required: true
          type: string
          format: string
          default: "15"
        - name: CancelDevicePlan
          in: body
          description: Cancel Device Plan
          required: true
          schema:
            $ref: '#/definitions/CancelDevicePlan_Request'
      responses:
        '200':
          description: Success Response
          schema:
            $ref: '#/definitions/CancelDevicePlan_Success_Response'
        default:
          description: unexpected error
          schema:
            $ref: '#/definitions/errorModel'

definitions:
  CancelDevicePlan_Request:
    type: object
    required:
      - ServiceRequestDetail
      - deviceInformation
      - accountId
    properties:
      ServiceRequestDetail:
        type: object
        properties:
          OwnerId:
            type: string
            default: "2282"
          ResponseType:
            type: string
            enum: ["JSON","XML"]
            default: "JSON"
          EndClientUserUniqueSessionId:
            type: string
            default: "1.0"
          UserName:
            type: string
            default: "hylaadmin"
          Token:
            type: string
            default: ""
          ServiceRequestVersion:
            type: string
            default: "1.0"
          ServiceResponseVersion:
            type: string
            default: "1.0"
          RegionCode:
            type: string
            default: "US"
          LanguageCode:
            type: string
            default: "en"
      accountId:
        type: string
        default: "1000000"
      deviceInformation:
        type: array
        items:
          properties:
            deviceId:
              title: "deviceId"
              type: string
              default: "654212121241121"
            deviceCancellationDate:
              title: "deviceCancellationDate"
              type: string
              default: "04/02/2020"
            cancelType:
              title: "cancelType"
              type: string
              default: "Prorata"
            cancelReason:
              title: "cancelReason"
              type: string
              default: "DEVICE_LOST"
            action:
              title: "action"
              type: string
              default: "Removed"
            cancellationTime:
              title: "cancellationTime"
              type: string
              default: "04/16/2020 13:08:00"
     
  CancelDevicePlan_Success_Response:
    type: object
    required:
      - ServiceRequestDetail
      - deviceInformation
      - accountId
    properties:
      EffectiveDate:
        type: string
      ServiceRequestDetail:
        type: object
        properties:
          ResponseType:
            type: string
            enum: ["JSON","XML"]
            default: "JSON"
          OwnerId:
            type: string
            default: "2282"
          EndClientUserUniqueSessionId:
            type: string
            default: "1.0"
          UserName:
            type: string
            default: "hylaadmin"
          Token:
            type: string
            default: ""
          ServiceRequestVersion:
            type: string
            default: "1.0"
          RegionCode:
            type: string
            default: "US"
          LanguageCode:
            type: string
            default: "en"
      accountId:
        type: string
        default: "1000000"
      ResponseStatus:
        type: string
        default: "Success"
      returnedOrders:
        type: string
      documents:
        type: array
        items:
          properties:
            name:
              title: "name"
              type: string
              default: "Invoice"
            URL:
              title: "URL"
              type: string
      
      deviceInformation:
              type: array
              items:
                properties:
                   deviceId:
                     title: "deviceId"
                     type: string
                     default: "654212121241121"
                   returnPremium:
                     title: "returnPremium"
                     type: string
                     default: "8"
                   deviceCancellationDate:
                     title: "planName"
                     type: string
                     default: "02/05/2020"
                   cancelType:
                     title: "effectiveDate"
                     type: string
                     default: "Prorata"
                   cancelReason:
                     title: "cancelReason"
                     type: string
                     default: "DEVICE_LOST"
                   cancellationTime:
                     title: "cancellationTime"
                     type: string
                     default: "04/16/2020 13:08:00"
                   documents:
                       type: array
                       items:
                         properties:
                          name:
                            title: "name"
                            type: string
                          URL:
                            title: "URL"
                            type: string
                   planDetails:
                       type: array
                       items:
                         properties:
                          type:
                            type: string
                          policyNumber:
                            type: string
                          policyStatus:
                            type: string
        
           
  errorModel:
    type: object
    required:
      - RequestStatus
      - MessageDetail
    properties:
      RequestStatus:
        type: string
      MessageDetail:
        type: object
        properties:
          MessageCode:
            type: string
          MessageType:
            type: string
          UserMessage:
            type: string
          MessageLevel:
            type: string
          MessageSeverity:
            type: string
          MessageList:
            type: object
            properties:
              Message:
                type: array
                items:
                  type: string    
